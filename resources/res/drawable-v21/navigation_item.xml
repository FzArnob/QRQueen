<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:aapt="http://schemas.android.com/aapt" android:color="#25000000">
    <item>
        <selector>
            <item android:state_checked="true">
                <shape android:shape="rectangle">
                    <solid android:color="#25000000"/>
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#ffffff"/>
                </shape>
            </item>
        </selector>
    </item>
</ripple>
